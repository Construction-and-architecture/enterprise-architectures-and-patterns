CREATE TABLE IF NOT EXISTS ITEMS (
   ID varchar(36) PRIMARY KEY,
   NAME TEXT NOT NULL
);

INSERT INTO ITEMS(ID, NAME)
SELECT '8b933c84-ee60-45a1-848d-428ad3259e2b', 'Green Apples'
WHERE
NOT EXISTS (
SELECT ID FROM ITEMS WHERE ID = '8b933c84-ee60-45a1-848d-428ad3259e2b'
);

INSERT INTO ITEMS(ID, NAME)
SELECT 'd660b9b2-0406-46d6-9efe-b40b4cca59fc', 'Bananas'
WHERE
NOT EXISTS (
SELECT ID FROM ITEMS WHERE ID = 'd660b9b2-0406-46d6-9efe-b40b4cca59fc'
);
